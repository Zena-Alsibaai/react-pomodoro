        break;\r\n            case \"shortBreak\":\r\n                setNewTimer({\r\n                    ...newTimer,\r\n                    short: parseInt(value),\r\n                });\r\n                break;\r\n            case \"longBreak\":\r\n                setNewTimer({\r\n                    ...newTimer,\r\n                    long: parseInt(value),\r\n                });\r\n            default:\r\n                break;\r\n        }\r\n        console.log(newTimer);\r\n    };\r\n\r\n    const handleSubmit = () => {};\r\n    return (\r\n        <div className=\"form-conatiner\">\r\n            <form noValidate>\r\n                <div className=\"input-wrapper\">\r\n                    <input\r\n                        className=\"input\"\r\n                        name=\"work\"\r\n                        onChange={handleChange}\r\n                        value={newTimer.work}></input>\r\n\r\n                    <input\r\n                        className=\"input\"\r\n                        name=\"shortBreak\"\r\n                        onChange={handleChange}\r\n                        value={newTimer.short}></input>\r\n\r\n                    <input\r\n                        className=\"input\"\r\n                        name=\"longBreak\"\r\n                        onChange={handleChange}\r\n                        value={newTimer.long}></input>\r\n                </div>\r\n                <Button title=\"Set Timer\" _callback={handleSubmit} />\r\n            </form>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default SetPomodoro;\r\n"},"lineCount":null}},"error":null,"hash":"1b95b99ebc776573c76ae8d16c91b8b8","cacheData":{"env":{}}}